<section id="<%= field.scroll_anchor_id%>" class="cta-section">
  <div class="container">
    <div class="row">
      <div class="col-lg-6 order-last order-lg-first">
        <div class="cta-image <%= field.image_position%>-image">
          <img src="<%= field.image %> " alt="" />
        </div>
      </div>
      <div class="col-lg-6">
        <div class="cta-content-wrapper">
          <div class="section-title">
            <h2 class="mb-20">
              <%= field.headline %>
            </h2>
            <%- field.subheadline%>
            <a href="<%= field.button_url %> " target="_blank" class="main-btn btn-hover border-btn mt-30">
              <%= field.button_label %>
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>