{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": "error",
    "prefer-destructuring": "off",
    "no-unused-expressions": "off"
  },
  "plugins": ["ejs"]
}
